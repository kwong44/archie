{
  "expo": {
    "name": "archie",
    "slug": "archie-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appstore.png",
    "splash": {
      "image": "./assets/images/appstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#121820"
    },
    "scheme": "archie",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.archie.app",
      "icon": "./assets/images/AppIcons/Assets.xcassets/AppIcon.appiconset/1024.png",
      "splash": {
        "image": "./assets/images/appstore.png",
        "resizeMode": "contain",
        "backgroundColor": "#121820",
        "tabletImage": "./assets/images/appstore.png"
      },
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app needs access to your microphone to record audio for your journal entries and personal reflections.",
        "NSCameraUsageDescription": "This app does not use the camera. This permission is required by iOS for certain app frameworks, but camera access is never requested.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.archie.app",
      "icon": "./assets/images/playstore.png",
      "splash": {
        "image": "./assets/images/playstore.png",
        "resizeMode": "contain",
        "backgroundColor": "#121820",
        "mdpi": "./assets/images/playstore.png",
        "hdpi": "./assets/images/playstore.png",
        "xhdpi": "./assets/images/playstore.png",
        "xxhdpi": "./assets/images/playstore.png",
        "xxxhdpi": "./assets/images/playstore.png"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AppIcons/android/res/mipmap-xxxhdpi/ic_launcher.png",
        "backgroundColor": "#121820"
      },
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.archie.app",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-splash-screen",
      [
        "expo-av",
        {
          "microphonePermission": "This app needs access to your microphone to record audio for your journal entries and personal reflections."
        }
      ],
      "expo-localization",
      "expo-updates"
    ],
    "updates": {
      "url": "https://u.expo.dev/2fa5ea09-ee3b-4740-be0a-56e083256c37"
    },
